<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews -Indexes
    </IfModule>

    RewriteEngine On

    RewriteCond %{HTTP:Accept-Language} ^es [NC]
    RewriteCond %{HTTP_HOST} !es.orxatasoftware\.com [NC]
    RewriteRule ^(.*)$ https://es.orxatasoftware.com
</IfModule>
